{
  "name": "greenfield",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ArchangelGabriel/greenfield",
  "author": "Juan Gabriel <juangabriel@pixeloak.com>",
  "browserslist": [
    "> 1%"
  ],
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib && babel src -d lib --ignore .test.js && export NODE_ENV=production && webpack -p --progress",
    "prod:start": "export NODE_ENV=production && pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && jest",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-compat": "^1.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "fetch-mock": "^5.11.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "pm2": "^2.5.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.10.7",
    "prop-types": "^15.5.10",
    "raven": "^2.0.2",
    "raven-for-redux": "^1.0.0",
    "raven-js": "^3.16.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0"
  }
}
